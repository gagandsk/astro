---
import { getCollection } from "astro:content";
import Base from "@layouts/Base.astro";
import Products from '@layouts/Products.astro'
const products = (await getCollection('products')).sort((a, b) => b.data.publicationDate.valueOf() - a.data.publicationDate.valueOf());
---

<Base title="Ladrillos.club">
	<Products product={products}/>
</Base>